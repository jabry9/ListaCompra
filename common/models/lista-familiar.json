{
  "name": "ListaFamiliar",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "Nombre": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "Propietario": {
      "type": "belongsTo",
      "model": "Usuario",
      "foreignKey": "Owner"
    },
    "productos": {
      "type": "hasMany",
      "model": "Producto",
      "foreignKey": ""
    },
    "Solicitud": {
      "type": "hasAndBelongsToMany",
      "model": "Usuario",
      "foreignKey": ""
    },
    "participantes": {
      "type": "hasMany",
      "model": "Producto",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "create"
    }
  ],
  "methods": {}
}
